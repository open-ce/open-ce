From 3852bfa515f40648b5c3823913445a501cc8df74 Mon Sep 17 00:00:00 2001
From: Deepali Chourasia <deepch23@in.ibm.com>
Date: Fri, 29 Sep 2023 03:15:09 +0000
Subject: [PATCH] Removed recipes for packages other than gym

---
 recipe/meta.yaml | 130 ++---------------------------------------------
 1 file changed, 4 insertions(+), 126 deletions(-)

diff --git a/recipe/meta.yaml b/recipe/meta.yaml
index 592648b..829dcd3 100644
--- a/recipe/meta.yaml
+++ b/recipe/meta.yaml
@@ -29,13 +29,14 @@ outputs:
     requirements:
       host:
         - python
-        - pip
+        - pip {{ pip }}
         - cloudpickle >=1.2.0
-        - numpy
+        - numpy {{ numpy }}
+        - setuptools {{ setuptools }}
       run:
         - python
         - cloudpickle >=1.2.0
-        - numpy
+        - numpy {{ numpy }}
         - importlib_metadata >=4.8.1  # [py<38]
     test:
       requires:
@@ -46,129 +47,6 @@ outputs:
       commands:
         # TODO: run test suite
 
-  - name: gym-all
-    requirements:
-      host:
-        - python
-      run:
-        - python
-        # exclude gym-atari package because the Atari-Py dependency
-        # has been replaced with ALE-py which has not yet been
-        # packaged in conda-forge, see conda-forge/staged-recipes#16488
-        # - {{ pin_subpackage('gym-atari', exact=True) }}
-        - {{ pin_subpackage('gym-box2d', exact=True) }}
-        - {{ pin_subpackage('gym-classic_control', exact=True) }}
-        # exclude some packages because their dependency (mujoco) has
-        # incompatible license, see below
-        #- {{ pin_subpackage('gym-mujoco', exact=True) }}
-        #- {{ pin_subpackage('gym-robotics', exact=True) }}
-        - {{ pin_subpackage('gym-toy_text', exact=True) }}
-        - {{ pin_subpackage('gym-other', exact=True) }}
-    test:
-      imports:
-        - gym
-
-  - name: gym-atari
-    build:
-      # the Atari-Py dependency has been replaced with ALE-Py
-      # which has not yet been packaged in conda-forge
-      skip: true
-    requirements:
-      host:
-        - python
-      run:
-        - python
-        - {{ pin_subpackage('gym', exact=True) }}
-        - ale-py >=0.7.1,<0.8
-    test:
-      imports:
-        - gym.envs.atari
-
-  - name: gym-box2d
-    requirements:
-      host:
-        - python
-      run:
-        - python
-        - {{ pin_subpackage('gym', exact=True) }}
-        - box2d-py ==2.3.8
-        - pyglet >=1.4.0
-    test:
-      imports:
-        - gym.envs.box2d
-
-  - name: gym-classic_control
-    requirements:
-      host:
-        - python
-      run:
-        - python
-        - pyglet >=1.4.0
-        - {{ pin_subpackage('gym', exact=True) }}
-        # no extra dependencies for this combination
-    test:
-      imports:
-        - gym.envs.classic_control
-
-  - name: gym-mujoco
-    build:
-      # mujoco-py cannot be packaged for license reasons, see
-      # https://github.com/openai/mujoco-py#install-mujoco
-      skip: true
-    requirements:
-      host:
-        - python
-      run:
-        - python
-        - {{ pin_subpackage('gym', exact=True) }}
-        - mujoco-py >=1.50,<2.0
-        - imageio
-    test:
-      imports:
-        - gym.envs.mujoco
-
-  - name: gym-robotics
-    build:
-      # mujoco-py cannot be packaged for license reasons, see
-      # https://github.com/openai/mujoco-py#install-mujoco
-      skip: true
-    requirements:
-      host:
-        - python
-      run:
-        - python
-        - {{ pin_subpackage('gym', exact=True) }}
-        - mujoco-py >=1.50,<2.0
-        - imageio
-    test:
-      imports:
-        - gym.envs.robotics
-
-  - name: gym-toy_text
-    requirements:
-      host:
-        - python
-      run:
-        - python
-        - {{ pin_subpackage('gym', exact=True) }}
-        - scipy >=1.4.1
-    test:
-      imports:
-        - gym.envs.toy_text
-
-  - name: gym-other
-    requirements:
-      host:
-        - python
-      run:
-        - python
-        - {{ pin_subpackage('gym', exact=True) }}
-        - lz4 >=3.1.0
-        - opencv >=3.0
-    test:
-      imports:
-        # There are no tests in the source code for gym-other
-
   # - name: gym-accept-rom-license
   #   requirements:
   #     host:
-- 

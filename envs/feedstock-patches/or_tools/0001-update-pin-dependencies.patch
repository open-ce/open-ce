From 1f439e0c80448b6f5d03999dd8bbc237526655bf Mon Sep 17 00:00:00 2001
From: Archana-Shinde1 <Archana-Shinde1@ibm.com>
Date: Thu, 5 Sep 2024 04:21:53 +0000
Subject: [PATCH] Update pin denpendencies

---
 recipe/meta.yaml | 32 +++++++++++++++++++-------------
 1 file changed, 19 insertions(+), 13 deletions(-)

diff --git a/recipe/meta.yaml b/recipe/meta.yaml
index 490cbb8..fde48c0 100644
--- a/recipe/meta.yaml
+++ b/recipe/meta.yaml
@@ -30,11 +30,12 @@ outputs:
         - cmake >=3.18
         - ninja
         - pkg-config
+        - libprotobuf {{ libprotobuf }}
       host:
         # eigen does not have a run-export but is header-only
-        - eigen
-        - libabseil
-        - libprotobuf
+        - eigen {{ eigen }}
+        - libabseil {{ abseil_cpp }}
+        - libprotobuf {{ libprotobuf }}
         - re2
         - zlib
         # coin or deps
@@ -47,6 +48,8 @@ outputs:
         # fake python dependency to avoid conda-smithy collapsing the builds per arch;
         # we can remove this once we manage to base the python bindings on libortools
         - python
+      run:
+        - openblas {{ openblas }}
       run_constrained:
         # make sure we don't co-install with old version of previous package name
         - ortools-cpp ={{ version }}
@@ -88,21 +91,23 @@ outputs:
         - python
         - mypy-protobuf
         - pkg-config
-        - setuptools
+        - setuptools {{ setuptools }}
         - swig >=4.0
+        - libprotobuf {{ libprotobuf }}
+        - protobuf {{ protobuf }}
       host:
         - python
         - pip
         - absl-py >=0.13
-        - numpy
+        - numpy {{ numpy }}
         - mypy-protobuf
-        - protobuf >=4.21.12
-        - scipy >=1.10
+        - protobuf {{ protobuf }}
+        - scipy {{ scipy }}
         - wheel
         # C/C++ deps
-        - eigen
-        - libabseil
-        - libprotobuf
+        - eigen {{ eigen }}
+        - libabseil {{ abseil_cpp }}
+        - libprotobuf {{ libprotobuf }}
         - pybind11
         - re2
         - zlib
@@ -116,9 +121,10 @@ outputs:
         - python
         - absl-py >=0.13
         - mypy-protobuf
-        - {{ pin_compatible('numpy') }}
-        - protobuf >=4.21.12
-        - scipy >=1.10
+        - numpy {{ numpy }}
+        - protobuf {{ protobuf }}
+        - scipy {{ scipy }}
+        - openblas {{ openblas }}
         # Source: https://github.com/google/or-tools/blob/stable/ortools/python/setup.py.in#L44
 
     test:
-- 
2.40.1


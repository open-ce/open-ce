From: Nishidha Panpaliya <npanpa23@in.ibm.com>
Date: Mon, 30 Jan 2023 16:54:43 +0000
Subject: [PATCH] P10 changes

---
 recipe/meta.yaml | 8 ++++++--
 1 file changed, 6 insertions(+), 2 deletions(-)

diff --git a/recipe/meta.yaml b/recipe/meta.yaml
index 70be850..acf577f 100644
--- a/recipe/meta.yaml
+++ b/recipe/meta.yaml
@@ -24,8 +24,8 @@ outputs:
     requirements:
       build:
         - gnuconfig  # [unix]
-        - {{ compiler('c') }}
-        - {{ compiler('cxx') }}
+        - {{ compiler('c') }}      #[ppc_arch != "p10"]
+        - {{ compiler('cxx') }}    #[ppc_arch != "p10"]
         - pkg-config
         - make  # [unix]
       host:
@@ -33,6 +33,9 @@ outputs:
         - coin-or-clp
         - coin-or-osi
         - coin-or-utils
+        - openblas {{ openblas }}
+      run:
+        - openblas {{ openblas }} 
       run_constrained:
         - coincbc * *_metapackage
 
@@ -69,6 +72,7 @@ outputs:
     requirements:
       run:
         - coin-or-cbc {{ version }}
+        - openblas {{ openblas }}
     test:
       commands:
         - test -f $PREFIX/lib/libCbc${SHLIB_EXT}
